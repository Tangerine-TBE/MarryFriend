<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SeekBarView">
        <!--        thumb是否隐藏，默认false，不隐藏-->
        <attr name="thumbHide" format="boolean" />
        <!--        是否禁用手指拖动，true 禁用， false默认值，不禁用-->
        <attr name="touchEnable" format="boolean" />
        <!--        thumb文字-->
        <attr name="thumbText" format="string" />
        <!--        thumb字体大小-->
        <attr name="thumbTextSize" format="dimension" />
        <!--        thumb图标-->
        <attr name="thumbIcon" format="reference|color" />
        <!--        thumb图标宽-->
        <attr name="thumbIconWidth" format="dimension" />
        <!--        thumb图标高-->
        <attr name="thumbIconHeight" format="dimension" />
        <!--        thumb字体颜色-->
        <attr name="thumbTextColor" format="color|reference" />
        <!--        thumb边框宽度-->
        <attr name="thumbBorderWidth" format="dimension" />
        <!--        thumb边框颜色-->
        <attr name="thumbBorderColor" format="color|reference" />
        <!--        thumb边框渐变开始颜色-->
        <attr name="thumbBorderStartColor" format="color|reference" />
        <!--        thumb边框渐变结束颜色-->
        <attr name="thumbBorderEndColor" format="color|reference" />
        <!--        thumb前后padding值，可以为负-->
        <attr name="headEndPadding" format="dimension" />
        <!--        前景进度条高度-->
        <attr name="prospectProgressBarHeight" format="dimension" />
        <!--        前景进度条开始的渐变颜色-->
        <attr name="prospectProgressBarStartColor" format="color|reference" />
        <!--        前景进度条颜色-->
        <attr name="prospectProgressBarColor" format="color|reference" />
        <!--        前景进度条结束的渐变颜色-->
        <attr name="prospectProgressBarEndColor" format="color|reference" />
        <!--        触发偏移量-->
        <attr name="prospectProgressBarOffset" format="dimension" />
        <!--        背景进度条高度-->
        <attr name="backgroundProgressBarHeight" format="dimension" />
        <!--        背景进度条渐变开始颜色-->
        <attr name="backgroundProgressBarStartColor" format="color|reference" />
        <!--        背景进度条颜色（如果设置为渐变，则，这个是中间的）-->
        <attr name="backgroundProgressBarColor" format="color|reference" />
        <!--        背景进度条渐变结束颜色-->
        <attr name="backgroundProgressBarEndColor" format="color|reference" />
        <!--        触发偏移量-->
        <attr name="backgroundProgressBarOffset" format="dimension" />
        <!--        thumb开始颜色-->
        <attr name="thumbBackgroundStartColor" format="color|reference" />
        <!--        thumb颜色-->
        <attr name="thumbBackgroundColor" format="color|reference" />
        <!--        thumb结束颜色-->
        <attr name="thumbBackgroundEndColor" format="color|reference" />
        <!--        thumb偏移量-->
        <attr name="thumbOffset" format="dimension" />
        <!--        thumb宽度-->
        <attr name="thumbWidth" format="dimension" />
        <!--        thumb高度-->
        <attr name="thumbHeight" format="dimension" />
        <!--        thumb圆角还是矩形-->
        <attr name="thumbType">
            <flag name="square" value="0x10" />
            <flag name="round" value="0x20" />
        </attr>
        <attr name="strokeCap">
            <flag name="butt" value="0x10" />
            <flag name="round" value="0x20" />
            <flag name="square" value="0x30" />
        </attr>
        <!--        thumb进度-->
        <attr name="progress" format="integer" />

    </declare-styleable>


    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>


    <!--线（进度条）宽度-->
    <attr name="lineHeight" format="dimension" />
    <!--字的大小 100元-->
    <attr name="textSize" format="dimension" />
    <!--字的颜色 100元-->
    <attr name="textColor" format="color" />
    <!--两个游标内部 线（进度条）的颜色-->
    <attr name="inColor" format="color" />
    <!--两个游标外部 线（进度条）的颜色-->
    <attr name="outColor" format="color" />
    <!--左边图标的图片-->
    <attr name="imageLow" format="reference" />
    <!--右边图标 的图片-->
    <attr name="imageBig" format="reference" />
    <!--游标 图片宽度-->
    <attr name="imagewidth" format="dimension" />
    <!--游标 图片高度-->
    <attr name="imageheight" format="dimension" />
    <!--是否有刻度线-->
    <attr name="hasRule" format="boolean" />
    <!--刻度的颜色-->
    <attr name="ruleColor" format="color" />
    <!--刻度上边的字 的颜色-->
    <attr name="ruleTextColor" format="color" />
    <!--单位 元-->
    <attr name="unit" format="string" />
    <!--单位份数-->
    <attr name="equal" format="integer" />
    <!--刻度单位 $-->
    <attr name="ruleUnit" format="string" />
    <!--刻度上边文字的size-->
    <attr name="ruleTextSize" format="dimension" />
    <!--刻度线的高度-->
    <attr name="ruleLineHeight" format="dimension" />
    <!--选择器的最大值-->
    <attr name="bigValue" format="integer" />
    <!--选择器的最小值-->
    <attr name="smallValue" format="integer" />

    <declare-styleable name="DoubleSlideSeekBar">
        <attr name="lineHeight" />
        <attr name="textSize" />
        <attr name="textColor" />
        <attr name="inColor" />
        <attr name="outColor" />
        <attr name="imageLow" />
        <attr name="imageBig" />
        <attr name="imagewidth" />
        <attr name="imageheight" />
        <attr name="hasRule" />
        <attr name="ruleColor" />
        <attr name="ruleTextColor" />
        <attr name="unit" />
        <attr name="equal" />
        <attr name="ruleUnit" />
        <attr name="ruleTextSize" />
        <attr name="ruleLineHeight" />
        <attr name="bigValue" />
        <attr name="smallValue" />
    </declare-styleable>

</resources>